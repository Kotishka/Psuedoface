{
    "name": "Psuedoface",
    "description": "A facial-cloaking extension software based on an open source project, Fawkes.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/Psuedoface-16.png",
            "32": "/images/Psuedoface-32.png",
            "48": "/images/Psuedoface-48.png",
            "128": "/images/Psuedoface-128.png"
        }
    },
    "icons": {
        "16": "/images/Psuedoface-16.png",
        "32": "/images/Psuedoface-32.png",
        "48": "/images/Psuedoface-48.png",
        "128": "/images/Psuedoface-128.png"
    }
}